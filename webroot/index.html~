		self.onmessage = function(e){
		   postMessage({ type: 'log', text: 'Starting to work.'});
		   
		   //do some work
		   i = 123;
		   postMessage({ type: 'log', text: 'Work done.'});
		   postMessage({ type: 'found', value: 'ThePassw0rd'+i});
		}
		</textarea>
